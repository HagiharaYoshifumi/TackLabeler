  <metadata>
    <id>NPOI</id>
    <version>2.5.1</version>
    <title>NPOI</title>
    <authors>NPOI Contributors</authors>
    <owners>Tony Qu</owners>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <license type="file">LICENSE</license>
